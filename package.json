{
  "name": "@motorcycle/firebase",
  "version": "0.0.0",
  "description": "Firebase Drivers for Motorcycle.js",
  "main": "lib/commonjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:lint": "tslint src/**/*.ts src/*.ts",
    "test:unit": "TS_NODE_PROJECT=test/tsconfig.json mocha -r ts-node/register test/*.ts",
    "test": "npm run test:lint && npm run test:unit",
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "postchangelog": "git add CHANGELOG.md && git commit -m 'docs(CHANGELOG): append to changelog'",
    "build:es2015": "tsc -P tsconfig.json",
    "build:commonjs": "tsc -P tsconfig.commonjs.json",
    "build": "npm run build:es2015 && npm run build:commonjs",
    "preversion": "npm run build",
    "postversion": "npm run changelog && git push origin master --tags && npm publish",
    "release:minor": "npm version minor -m 'chore(package): v%s'",
    "release:major": "npm version major -m 'chore(package): v%s'"
  },
  "repository": {
    "type": "git",
    "url": "https:/github.com/motorcyclejs/firebase"
  },
  "keywords": [
    "motorcycle",
    "cycle",
    "most",
    "firebase",
    "driver",
    "storage",
    "database",
    "authentication"
  ],
  "author": "Tylor Steinberger <tlsteiberger167@gmail.com>",
  "license": "MIT",
  "config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    }
  },
  "jsnext:main": "lib/es2015/index.js",
  "module": "lib/es2015/index.js",
  "typings": "lib/es2015/index.d.ts",
  "devDependencies": {
    "@motorcycle/tslint": "^1.2.0",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.49",
    "commitizen": "^2.8.6",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "mocha": "^3.1.2",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.1",
    "typescript": "^2.2.0-dev.20161122",
    "validate-commit-msg": "^2.8.2"
  },
  "dependencies": {
    "@most/hold": "^1.3.1",
    "firebase": "^3.6.1",
    "most": "^1.1.0"
  }
}
